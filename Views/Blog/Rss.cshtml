<?xml version="1.0" ?>
<rss version="2.0">
<channel>
<title>Primoca Blog</title>
<link>http://blog.primoca.com/</link>
<description>Thoughts from the closet office</description>
<pubDate>@DateTime.Now.ToString("ddd, dd MMM yyyy HH:mm:ss") @DateTime.Now.ToString("zzzz").Replace(":", "")</pubDate>
<lastBuildDate>@DateTime.Now.ToString("ddd, dd MMM yyyy HH:mm:ss") @DateTime.Now.ToString("zzzz").Replace(":", "")</lastBuildDate>
@foreach (var item in Model)
{
	@:<item>
		@:<title>@item.Title</title>
		@:<link>http://blog.primoca.com/posts/@item.FullId</link>
		@:<description>
			<![CDATA[
			@item.HtmlText
			]]>
		@:</description>
		@:<pubDate>@item.Date.ToString("ddd, dd MMM yyyy HH:mm:ss") @item.Date.ToString("zzzz").Replace(":", "")</pubDate>
		@:<guid>http://blog.primoca.com/posts/@item.FullId</guid>
	@:</item>
}
</channel>
</rss>
@{
	Layout = null;
	Response.ContentType = "application/rss+xml";
}
@model  IEnumerable<Static_Blog.Models.Post>
